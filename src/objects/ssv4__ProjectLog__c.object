    <validationRules>
        <fullName>ssv4__checkBlockers</fullName>
        <active>true</active>
        <description>Check blocker and blocker Reason</description>
        <errorConditionFormula>( ISPICKVAL( ssv4__Status__c , &apos;Blocked&apos;)  &amp;&amp;  ISBLANK(ssv4__blockReason__c ))</errorConditionFormula>
        <errorDisplayField>ssv4__blockReason__c</errorDisplayField>
        <errorMessage>Please enter a blocker reason</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ssv4__checkStepToReproduce</fullName>
        <active>true</active>
        <errorConditionFormula>$RecordType.DeveloperName = &apos;bug&apos; &amp;&amp;  LEN( ssv4__stepToReproduce__c ) &lt; 20</errorConditionFormula>
        <errorDisplayField>ssv4__stepToReproduce__c</errorDisplayField>
        <errorMessage>Please provide step to reproduce information or another information about this bug</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ssv4__checkTimeValue</fullName>
        <active>true</active>
        <description>Allow only chars [0..9mMhH ]</description>
        <errorConditionFormula>NOT(
REGEX(
ssv4__appEstimationTxt__c,
&quot;(?i)([0123456789mh ])+?&quot;
)
) &amp;&amp;  NOT(ISBLANK(ssv4__appEstimationTxt__c))</errorConditionFormula>
        <errorDisplayField>ssv4__appEstimationTxt__c</errorDisplayField>
        <errorMessage>Please enter correct value. Allow only chars [0..9mMhH ]</errorMessage>
    </validationRules>
