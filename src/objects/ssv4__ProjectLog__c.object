        <fullName>ssv4__appEstimationTxt__c</fullName>
        <deprecated>false</deprecated>
        <externalId>false</externalId>
        <inlineHelpText>Approximaly Estimation in minutes(s) for this task. You can use [d|D] - days, [h|H] - hours, [m|M] - minutes</inlineHelpText>
        <label>Approximaly Estimation</label>
        <length>10</length>
        <required>false</required>
        <trackHistory>true</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>ssv4__appEstimation__c</fullName>
        <deprecated>false</deprecated>
        <externalId>false</externalId>
        <formula>IF(ISBLANK(ssv4__appEstimationTxt__c), 0,
	VALUE(&apos;0&apos;+
		MID(
			SUBSTITUTE(SUBSTITUTE(LOWER(SUBSTITUTE(ssv4__appEstimationTxt__c, &apos; &apos;, &apos;&apos;)), &apos;h&apos;, &apos;+&apos;), &apos;m&apos;, &apos;&apos;),
			0,
			FIND(&apos;+&apos;, SUBSTITUTE(SUBSTITUTE(LOWER(SUBSTITUTE(ssv4__appEstimationTxt__c, &apos; &apos;, &apos;&apos;)), &apos;h&apos;, &apos;+&apos;), &apos;m&apos;, &apos;&apos;)) - 1
		)
	)*60 +
VALUE(&apos;0&apos;+
		MID(
			SUBSTITUTE(SUBSTITUTE(LOWER(SUBSTITUTE(ssv4__appEstimationTxt__c, &apos; &apos;, &apos;&apos;)), &apos;h&apos;, &apos;+&apos;), &apos;m&apos;, &apos;&apos;),
			FIND(&apos;+&apos;, SUBSTITUTE(SUBSTITUTE(LOWER(SUBSTITUTE(ssv4__appEstimationTxt__c, &apos; &apos;, &apos;&apos;)), &apos;h&apos;, &apos;+&apos;), &apos;m&apos;, &apos;&apos;)) + 1,10
		)
	)
)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Approximaly Estimation</label>
        <precision>18</precision>
        <required>false</required>
        <scale>0</scale>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Number</type>
        <unique>false</unique>
